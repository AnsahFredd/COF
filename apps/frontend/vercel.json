{
  "version": 2,
  "builds": [
    {
      "src": "apps/frontend/package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["apps/frontend/**/*"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/frontend/dist/$1"
    }
  ],
  "build": {
    "env": {},
    "nodeVersion": "18.x"
  }
}
